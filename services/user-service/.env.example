# Build from same keys as repo root .env (POSTGRES_*, REDIS_PASSWORD, JWT_*).
# Local: use localhost. Docker: use service name (postgres, redis).
DATABASE_URL=postgresql://smartcourse:smartcourse_secret@localhost:5432/smartcourse
REDIS_URL=redis://:smartcourse_secret@localhost:6379/0

JWT_SECRET_KEY=your-super-secret-jwt-key-change-this-in-production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=15
JWT_REFRESH_TOKEN_EXPIRE_DAYS=7
